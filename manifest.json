{
    "name": "Custom Scripts Extenstion",
    "manifest_version": 2,
    "version": "1.0.0",
    "description": "Extenstion that help you run scripts at Google Chrome tabs",
    "icons": {
        "128": "assets/icon.svg"
    },
    "permissions": [
        "activeTab",
        "tabs",
        "debugger"
    ],
    "browser_action": {
        "default_icon": "assets/icon.png",
        "default_popup": "dist/index.html"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ]
}
